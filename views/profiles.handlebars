<div class="wrapper profiles-wrapper">
    <div class="profiles-banner">
        <h2>Profiler</h2>
    </div>

    <form id="profiles-search-form" onsubmit="event.preventDefault(); validateSearchForm()" method="get">
        <input name="profileName" type="text" placeholder="Sök profil... ">
        <input type="submit" value="Sök">
    </form>
    <section class="profile-list-wrapper">
        <ul>
            {{#if horses}}
                {{#each horses as |horse i|}}
                        <li>
                            <a href="/profile/{{horse}}">{{horse}}</a>
                        </li>
                        <li>
                            <a href="/profile/{{horse}}">{{horse}}</a>
                        </li>
                        <li>
                            <a href="/profile/{{horse}}">{{horse}}</a>
                        </li>
                        <li>
                            <a href="/profile/{{horse}}">{{horse}}</a>
                        </li>
                        <li>
