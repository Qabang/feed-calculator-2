                        </li>
                        <li>
                            <a href="/profile/{{horse}}">{{horse}}</a>
                        </li>
                        <li>
                            <a href="/profile/{{horse}}">{{horse}}</a>
                        </li>
                        <li>
                            <a href="/profile/{{horse}}">{{horse}}</a>
                        </li>
                        <li>
                            <a href="/profile/{{horse}}">{{horse}}</a>
                        </li>
                        <li>
                            <a href="/profile/{{horse}}">{{horse}}</a>
                        </li>
                        <li>
                            <a href="/profile/{{horse}}">{{horse}}</a>
                        </li>
                {{/each}}
            {{else}}
                <li class="no-profile">Tyvärr hittar vi inga profiler till din sökning.</li>
            {{/if}}
        </ul>
    </section>
</div>


